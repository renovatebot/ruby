{
  "name": "ruby-builds",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "repository": "https://github.com/renovatebot/ruby.git",
  "author": "Michael Kriese <michael.kriese@visualon.de>",
  "scripts": {
    "compile": "run-p compile:*",
    "compile:builder": "cd src/builder && ncc build ./index.ts -s -o ../../dist/builder",
    "compile:releaser": "cd src/releaser && ncc build ./index.ts -s -o ../../dist/releaser",
    "eslint": "eslint . --ext .js,.mjs,.ts",
    "eslint:fix": "yarn eslint --fix",
    "lint": "run-s eslint prettier",
    "prepare": "run-s compile:builder",
    "prettier": "prettier \"{**/*,*}.*\" --list-different",
    "prettier:fix": "prettier \"{**/*,*}.*\" --write",
    "start:builder": "run-s compile:builder && node dist/builder",
    "start:releaser": "run-s compile:releaser && node dist/releaser"
  },
  "dependencies": {
    "@actions/core": "1.2.6",
    "@actions/exec": "1.0.4",
    "@actions/github": "4.0.0",
    "@actions/io": "1.0.2",
    "@octokit/webhooks": "7.15.1",
    "@sindresorhus/is": "4.0.0",
    "chalk": "4.1.0",
    "renovate": "23.66.1",
    "shelljs": "0.8.4",
    "source-map-support": "0.5.19",
    "strip-ansi": "6.0.0",
    "www-authenticate": "0.6.2"
  },
  "devDependencies": {
    "@octokit/types": "5.5.0",
    "@types/jest": "26.0.15",
    "@types/node": "12.19.4",
    "@types/shelljs": "0.8.8",
    "@typescript-eslint/eslint-plugin": "4.6.0",
    "@typescript-eslint/parser": "4.6.0",
    "@zeit/ncc": "0.22.3",
    "eslint": "7.12.1",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-promise": "4.2.1",
    "husky": "4.3.0",
    "jest": "26.6.3",
    "nock": "13.0.5",
    "npm-run-all": "4.1.5",
    "prettier": "2.1.2",
    "prettier-plugin-package": "1.3.0",
    "pretty-quick": "3.1.0",
    "ts-jest": "26.4.4",
    "typescript": "4.0.5"
  }
}
